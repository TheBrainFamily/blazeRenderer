{
  "name": "blaze-renderer",
  "description": "We render blaze templates for you... so Meteor doesn't have to!",
  "version": "0.9.0",
  "scripts": {
    "start": "meteor run",
    "build": "babel blazeRenderer --out-dir lib --source-maps",
    "watch": "npm run build -- --watch",
    "test": "jest",
    "jest-html": "jest-html",
    "test:watch": "jest --watch"
  },
  "main": "lib/renderBlaze.js",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "glob": "^7.1.2",
    "js-beautify": "^1.7.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015-without-strict": "0.0.4",
    "babel-runtime": "^6.20.0",
    "jest": "^21.1.0",
    "jest-blaze-html": "^1.0.1"
  },
  "keywords": [
    "meteor",
    "blaze",
    "testing",
    "ssr"
  ]
}
